#:kivy 1.11.1
#:import rgba kivy.utils.rgba

<DesignElements>:
    
    FloatLayout:
        size: root.width, root.heigth

        canvas.before:
            Color:
                rgba: rgba('#0a5e00')
            Rectangle:
                size: self.size

        Carousel:
            direction: "right"
            loop: 'True'
            id: crsMain

            Label:
                text: "Members"
   
            Image:
                source: 'Resources/OfficialLogo.png'
                size: self.texture_size

            Label:
                id: lblName
                text: "Bierla"
            
